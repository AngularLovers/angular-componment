@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

[class^="{{classPrefix}}"], [class*=" {{classPrefix}}"]{
	font-family: {{fontName}};
	speak: none;
	line-height: 1;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}